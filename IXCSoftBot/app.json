{
  "name": "IXCSoftBot",
  "description": "Bot de músicas amiguinhos",
  "repository": "https://github.com/SudhanPlayz/Discord-MusicBot",
  "logo": "",
  "keywords": [
    "node",
    "discord",
    "youtube",
    "music",
    "bot",
    "lavalink",
    "dashboard"
  ],
  "image": "heroku/nodejs",
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "Prefix": {
      "description": "Prefixo do bot",
      "required": "true"
    },
    "Token": {
      "description": "Token do Bot (https://discord.com/developers/applications)",
      "required": "true"
    },
    "Discord_ClientID": {
      "description": "Client ID do bot",
      "required": "true"
    },
    "Discord_ClientSecret": {
      "description": "Client secret do bot",
      "required": "true"
    },
    "Website": {
      "description": "URL do webserver (Example: https://domain.xyz). Altere se você for usar a web dashboard.",
      "value": "http://localhost"
    },
    "Spotify_ClientID": {
      "description": "Spotify ClientID (https://developer.spotify.com/dashboard/)",
      "required": "true"
    },
    "Spotify_ClientSecret": {
      "description": "Spotify ClientSecret (https://developer.spotify.com/dashboard/)",
      "required": "true"
    }
  }
}
